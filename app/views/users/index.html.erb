<h1>User Admin</h1>

<table>
  <tr>
    <th>ID #</th>
    <th>Username</th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.username %></td>
    <td><%= link_to 'Show', user %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path, :class => "new_button" %>